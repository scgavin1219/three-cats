<h1>Specific Smelly Cat</h1>

<table>
    <thead>
        <tr>
            <th colspan="2">Smelly Cat</th>
        </tr>
    </thead>

    <tbody>
        <tr>
            <td>Name</td>
            <td><%= @cat.name %></td>
        </tr>

        <tr>
            <td>Age</td>
            <td><%= @cat.age %></td>
        </tr>

        <tr>
            <td>Sex</td>
            <td><%= @cat.sex %></td>
        </tr>

        <tr>
            <td>Color</td>
            <td><%= @cat.color %></td>
        </tr>

         <tr>
            <td>Description</td>
            <td><%= @cat.description %></td>
        </tr>

    </tbody>


</table>

<a href="<%= cats_url %>">Back to all the Cats</a>
