<h1>All the local cats we found</h1>
<ul>

    <% @cats.each do |cat| %>
    <br>
        <li><%= cat.name %></li>
        <li><%= cat.age %></li>
        <li><%= cat.color %></li>
        <li><%= cat.sex %></li>
        <li><%= cat.birth_date %></li>
        <li><%= cat.description %></li>
        <a href="<%= cat_url(cat)%>">Show the Cat</a>
    <% end %>

</ul>